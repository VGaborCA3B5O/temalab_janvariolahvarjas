@model WebApplication.Data.Entities.Post

<div class="col-sm-6 col-md-4 col-lg-3 mb-3">
    <div class="card">
        <img class="card-img-top" src="~/Images/MEMES/@(Model.Id).jpg" />
        <div class="card-body">
            <div class="card-title">
                <a>@Model.User</a>
            </div>
            <div class="card-text">
                <a>@Model.Content</a>
            </div>

        </div>
    </div>
</div>
